<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cydroid.powersaver.launcher"
    android:sharedUserId="android.uid.system"
    android:versionCode="90000000"
    android:versionName="9.0.0.at">

    <uses-permission android:name="android.permission.STATUS_BAR" />
    <uses-permission android:name="android.permission.REMOVE_TASKS" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_SECURE_SETTINGS" />
    <uses-permission android:name="android.permission.EXPAND_STATUS_BAR" />
    <!-- Gionee xionghg add for power saving optimization 145357 begin -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- Gionee xionghg add for power saving optimization 145357 end -->
    <application
        android:name=".PowersaverLauncherApp"
        android:allowBackup="false"
        android:enabled="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:maxAspectRatio="2.1"
        android:supportsRtl="true"
        android:theme="@style/PowerLauncherTheme">

        <!-- Gionee <yangxinruo> <2015-11-26> <modify> for CR01597494 begin -->
        <!-- Gionee <yangxinruo> <2015-8-18> <modify> for CR01539507 begin -->
        <!-- Chenyee <guoxt> <2018-03-13> <modify> for CSW1703A-821 begin -->
        <activity
            android:name=".Main"
            android:clearTaskOnLaunch="true"
            android:configChanges="keyboardHidden|mnc|mcc"
            android:excludeFromRecents="false"
            android:launchMode="singleInstance"
            android:screenOrientation="nosensor"
            android:stateNotNeeded="true"
            android:theme="@style/PowerLauncherTheme"
            android:windowSoftInputMode="adjustNothing">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.HOME" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="com.cydroid.powersaver.launcher" />
            </intent-filter>
        </activity>
        <!-- Chenyee <guoxt> <2018-03-13> <modify> for CSW1703A-821 end -->
        <!-- Gionee xionghg add for power saving optimization 145357 begin -->
        <!-- Chenyee guoxt modify for CSW1703A-1295 begin -->
        <activity
            android:name=".AppPickerActivity"
            android:label="@string/activity_title_add_apps"
            android:screenOrientation="nosensor"
            android:theme="@style/PowerLauncherSettingsTheme"></activity>

        <activity
            android:name=".settings.SettingActivity"
            android:excludeFromRecents="true"
            android:label="@string/settings_activity_title"
            android:launchMode="singleTask"
            android:theme="@style/PowerLauncherSettingsTheme"></activity>
        <!-- android:noHistory="true" -->
        <!-- Gionee xionghg add for power saving optimization 145357 end -->
        <!-- Chenyee guoxt modify for CSW1703A-1295 end -->

        <!-- Gionee <yangxinruo> <2015-8-18> <modify> for CR01539507 end -->
        <!-- Gionee <yangxinruo> <2015-11-26> <modify> for CR01597494 end -->
    </application>

</manifest>