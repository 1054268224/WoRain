<?xml version="1.0" encoding="utf-8"?>

<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <ImageButton android:id="@+id/cyee_increment"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:src="@color/cyee_transparent"
        android:background="@null"
        android:paddingTop="16dip"
        android:paddingBottom="22dip" />

    <view class="cyee.widget.CyeeNumberPicker$CustomEditText"
        android:id="@+id/cyee_numberpicker_input"
        style="?attr/cyeeNumberPickerTextStyle"
        android:layout_width="144dp"
        android:layout_height="wrap_content"
        android:background="@null"
        android:paddingTop="13sp"
        android:paddingBottom="13sp"
        android:paddingStart="2sp"
        android:paddingEnd="2sp"
        android:gravity="center"
        android:layout_gravity="center_horizontal"
        android:singleLine="true" />
    <!-- android:textColor="?android:attr/colorAccent"-->
    <!--shaozj android:textColor="@color/cyee_numberpicker_enable" -->
        <!--         android:textColor="?android:attr/textColorPrimary"      -->

    <ImageButton android:id="@+id/cyee_decrement"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:src="@color/cyee_transparent"
        android:background="@null"
        android:paddingTop="16dip"
        android:paddingBottom="22dip" />

</merge>
