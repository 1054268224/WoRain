<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <style name="TextAppearance.Cyee.Dark.Widget.ActionBar.Title" parent="@style/TextAppearance.Cyee.Light.Widget.ActionBar.Title">
        <item name="android:textColor">@color/cyee_actionbar_title_color_light</item>
    </style>

    <style name="TextAppearance.Cyee.Dark.Widget.ActionBar.Subtitle" parent="@style/TextAppearance.Cyee.Light.Widget.ActionBar.Subtitle">
        <item name="android:textColor">@color/cyee_actionbar_title_color_light</item>
    </style>
    
    <style name="TextAppearance.Cyee.Dark.DialogWindowTitle" parent="@android:style/TextAppearance.Holo.DialogWindowTitle">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/cyee_alert_dialog_title_light</item>
    </style>    
    
    <style name="Widget.Cyee.Dark.Button" parent="@style/Widget.Cyee.Light.Button">
        <item name="android:background">@drawable/cyee_btn_default_dark</item>
        <item name="android:textColor">@color/cyee_primary_text_dark</item> 
    </style>
    
    <style name="Widget.Cyee.Dark.Button.Small" parent="@style/Widget.Cyee.Light.Button.Small">
        <item name="android:background">@drawable/cyee_btn_default_small_dark</item>
    </style>
    
    <style name="Widget.Cyee.Dark.Button.Borderless" parent="@style/Widget.Cyee.Dark.Button"></style>
    
    <style name="Widget.Cyee.Dark.ProgressBar.Small" parent="@style/Widget.Cyee.Light.ProgressBar.Small">
        <item name="android:indeterminateDrawable">@drawable/cyee_progress_small_dark</item>
    </style>

    <style name="Widget.Cyee.Dark.ProgressBar.Large" parent="@style/Widget.Cyee.Light.ProgressBar.Large">
        <item name="android:indeterminateDrawable">@drawable/cyee_progress_large_dark</item>
    </style>  
        
    <style name="Widget.Cyee.Dark.ProgressBar.Small.Title"></style>
    <style name="Widget.Cyee.Dark.ProgressBar.Small.Inverse"></style>
    <style name="Widget.Cyee.Dark.ProgressBar.Large.Inverse"></style>
    
    <style name="Widget.Cyee.Dark.SeekBar" parent="@style/Widget.Cyee.Light.SeekBar">
        <item name="android:progressDrawable">@drawable/cyee_scrubber_progress_horizontal_dark</item>
        <item name="android:indeterminateDrawable">@drawable/cyee_scrubber_progress_horizontal_dark</item>
        <item name="android:thumb">@drawable/cyee_scrubber_control_selector_dark</item>
    </style>    
        
    <style name="Widget.Cyee.Dark.Spinner" parent="@style/Widget.Cyee.Light.Spinner">
        <item name="android:background">@drawable/cyee_spinner_background_dark</item>
        <item name="android:dropDownSelector">@drawable/cyee_list_selector_dark</item>
        <item name="android:popupBackground">@drawable/cyee_menu_dropdown_panel_dark</item>
    </style>
    
    <style name="Widget.Cyee.Dark.Spinner.DropDown"></style>  
    
    <style name="Widget.Cyee.Dark.Spinner.DropDown.ActionBar">
        <item name="android:background">@drawable/cyee_spinner_ab_light</item>
        <item name="android:dropDownWidth">304dip</item>
    </style>     
    
    <style name="Widget.Cyee.Dark.DropDownItem" parent="@style/Widget.Cyee.Light.DropDownItem">
        <item name="android:textAppearance">@style/TextAppearance.Cyee.Medium</item>
    </style>    
    
    <style name="Widget.Cyee.Dark.DropDownItem.Spinner"></style>
    
    <style name="Widget.Cyee.Dark.TabWidget" parent="@style/Widget.Cyee.Light.TabWidget">
    </style>
    
    <style name="Widget.Cyee.Dark.ActionBar" parent="@style/Widget.Cyee.Light.ActionBar">
        <item name="cyeetitleTextStyle">@style/TextAppearance.Cyee.Dark.Widget.ActionBar.Title</item>
        <item name="cyeesubtitleTextStyle">@style/TextAppearance.Cyee.Dark.Widget.ActionBar.Subtitle</item>
        <item name="cyeebackground">@drawable/cyee_cab_background_top_dark</item>
        <item name="cyeebackgroundStacked">@drawable/cyee_cab_background_top_dark</item>
        <item name="cyeebackgroundSplit">@drawable/cyee_cab_background_bottom_dark</item>
        <item name="cyeeprogressBarStyle">@style/Widget.Cyee.ProgressBar.Horizontal</item>
        <item name="cyeeindeterminateProgressStyle">@style/Widget.Cyee.ProgressBar</item>
    </style>
    
    <style name="Widget.Cyee.Dark.ActionBar.Solid">
        <item name="cyeedivider">?android:attr/dividerVertical</item>
        <item name="cyeeprogressBarPadding">@dimen/cyee_progressBar_Padding</item>
        <item name="cyeeitemPadding">@dimen/cyee_item_Padding</item>
    </style>
    
    <style name="Widget.Cyee.Dark.ListPopupWindow" parent="@style/Widget.Cyee.Light.ListPopupWindow">
        <item name="android:dropDownSelector">@drawable/cyee_list_selector_dark</item>
        <item name="android:popupBackground">@drawable/cyee_menu_dropdown_panel_dark</item>
    </style>
    
    <style name="Widget.Cyee.Dark.ExpandableListView" parent="@style/Widget.Cyee.Light.ExpandableListView">
        <item name="android:groupIndicator">@drawable/cyee_expander_group_dark</item>
    </style>
    
    <style name="CyeeAlertDialog.Dark">
        <item name="cyeefullDark">@drawable/cyee_dialog_full_dark</item>
        <item name="cyeetopDark">@drawable/cyee_dialog_top_dark</item>
        <item name="cyeecenterDark">@drawable/cyee_dialog_middle_dark</item>
        <item name="cyeebottomDark">@drawable/cyee_dialog_bottom_dark</item>
        <item name="cyeefullBright">@drawable/cyee_dialog_full_dark</item>
        <item name="cyeetopBright">@drawable/cyee_dialog_top_dark</item>
        <item name="cyeecenterBright">@drawable/cyee_dialog_middle_dark</item>
        <item name="cyeebottomBright">@drawable/cyee_dialog_bottom_dark</item>
        <item name="cyeebottomMedium">@drawable/cyee_dialog_bottom_dark</item>
        <item name="cyeecenterMedium">@drawable/cyee_dialog_middle_dark</item>        
    </style>    
    
    <style name="DialogWindowTitle.Cyee.Dark" parent="@style/DialogWindowTitle">
        <item name="android:textAppearance">@style/TextAppearance.Cyee.Dark.DialogWindowTitle</item>
    </style>
</resources>